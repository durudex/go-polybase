collection Full {
    id: string;
    name?: string;

    @index(id, [name, desc]);

    constructor (id: string, name?: string) {
        this.id = id;
        this.name = name;
    }

    update(name: string) {
        if (this.name == name) {
            throw error('you are already using this name');
        }

        this.name = name;
    }
}
